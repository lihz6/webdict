{
  "extends": "tslint:recommended",
  "rules": {
    "max-line-length": {
      "options": [
        1024
      ]
    },
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": false,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "no-console": {
      "options": [
        "debug",
        "info",
        "time",
        "timeEnd",
        "trace"
      ]
    },
    "quotemark": [
      true,
      "single",
      "avoid-escape",
      "avoid-template"
    ],
    "curly": [
      true,
      "ignore-same-line"
    ],
    "no-namespace": [
      false
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "max-classes-per-file": false,
    "member-access": false,
    "no-empty": [
      true,
      "allow-empty-catch"
    ],
    "no-shadowed-variable": [
      false
    ],
    "member-ordering": false,
    "one-variable-per-declaration": [
      false
    ],
    "object-literal-sort-keys": [
      false
    ],
    "prefer-for-of": true
  },
  "jsRules": {
    "extends": "jslint:recommended",
    "max-line-length": {
      "options": [
        1024
      ]
    }
  }
}
